@page "~/renderqrcode/{content?}"
@model RenderQrCodeModel
@{
    ViewData["Title"] = "Rendering the QrCode";
}
<h1>@ViewData["Title"]</h1>

<div class="text-center">
  <img src=@Model.imageSource class="rounded" alt="QRCode" width="400" height="400">
</div>

<div class="col-12">
    <a asp-page="GenerateQrCode"><button class="btn btn-primary">To Go Back</button></a>
</div>